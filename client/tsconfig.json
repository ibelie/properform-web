{
	"compilerOptions": {
		"module": "commonjs",
		"lib": [
			"dom",
			"es5",
			"es6",
			"scripthost",
			"dom.iterable"
		],
		"target": "es5",
		"alwaysStrict": true,
		"strictNullChecks": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": [
		"**/*"
	],
	"exclude": [
		"lib",
		"node_modules"
	]
}
