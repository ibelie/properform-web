{
	"compilerOptions": {
		"module": "commonjs",
		"lib": [
			"dom",
			"es5",
			"es6",
			"scripthost",
			"dom.iterable"
		],
		"target": "es5",
		"alwaysStrict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"strictNullChecks": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": [
		"../closure/closure.d.ts",
		"**/*"
	]
}
